{
  "version": 2,
  "framework": "astro",
  "installCommand": "git lfs install --local && git lfs fetch --all && git lfs checkout && npm ci",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
